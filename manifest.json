{
  "manifest_version": 3,
  "name": "LinkedIn Profile Enhancer",
  "version": "1.0",
  "description": "Injects company match data into LinkedIn profiles.",
  "permissions": ["storage", "scripting"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}